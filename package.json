{
    "name": "with-knex",
    "version": "0.1.0",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "migrate:make": "knex migrate:make",
        "migrate:latest": "knex migrate:latest",
        "migrate:up": "knex migrate:up",
        "migrate:down": "knex migrate:down",
        "migrate:status": "knex migrate:status",
        "seed:create": "knex seed:make",
        "seed:run": "knex seed:run",
        "format": "prettier --write .",
        "lint": "eslint . --ext jsx --ext js --resolve-plugins-relative-to .",
        "db:start": "docker-compose -f docker/docker-compose.yml up -d",
        "db:stop": "docker-compose -f docker/docker-compose.yml down",
        "db:remove": "docker-compose -f docker/docker-compose.yml rm"
    },
    "dependencies": {
        "eslint": "7.23.0",
        "eslint-config-prettier": "8.1.0",
        "eslint-plugin-react": "7.23.1",
        "jsonwebtoken": "8.5.1",
        "knex": "0.95.4",
        "next": "10.1.3",
        "next-connect": "0.10.1",
        "pg": "8.5.1",
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "license": "MIT",
    "devDependencies": {
        "prettier": "2.2.1"
    }
}
